{
  "name": "matrix-web-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:server\"",
    "dev:ui": "vite",
    "dev:server": "nodemon server/index.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently \"npm run start:ui\" \"npm run start:server\"",
    "start:ui": "vite preview --host",
    "start:server": "node server/index.js",
    "start:prod": "concurrently \"npm run start:ui:prod\" \"npm run start:server:prod\"",
    "start:ui:prod": "vite preview --host",
    "start:server:prod": "NODE_ENV=production node server/index.js"
  },
  "dependencies": {
    "@use-gesture/react": "^10.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.9",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
